local MainButton = DioPage.AddButton("TrashTalk#1", function()
    game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Shut yo goofy ahh up", "All")
    wait(1)
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Sit down son", "All")
         wait(1)
        game.ReplicatedStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("Yo neck look like a chug jug from fortnite", "All")
